#empty file handling

# clean up if the file exists.
if "testfile.txt" in listfiles():
  removefile("testfile.txt")

# create a file
myfile=openfile("testfile.txt",True)

try:
 assert("Hello There" == myfile.readat(0,0))
except:
 log("Empty file not handled! Accuracy Bug")
finally:
#Close the file
 myfile.close()