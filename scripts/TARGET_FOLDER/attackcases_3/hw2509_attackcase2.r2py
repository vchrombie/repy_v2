if "testfile2.txt" in listfiles():
  removefile("testfile2.txt")

myfile=openfile("testfile2.txt",True)

myfile.writeat("HELLO",0)

myfile.writeat("Hi!",5) 

myfile.writeat("BYE",8) 


# trying undo after undo
try:
  myfile.undo()

  myfile.undo()
except:
  pass

assert("HELLOHi!BYE" == myfile.readat(11,0))

myfile.close()