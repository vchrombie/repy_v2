# clean up if the file exists.
if "out.txt" in listfiles():
    removefile("out.txt")


myfile = openfile("out.txt", True)

myfile.undo()

myfile.writeat("hello", 0)

myfile.writeat("world", 5)

myfile.close()

myfile = openfile("out.txt", False)

assert("helloworld" == myfile.readat(None, 0))

myfile.close()